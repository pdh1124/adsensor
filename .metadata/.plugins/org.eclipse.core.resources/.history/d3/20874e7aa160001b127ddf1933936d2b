
public class data {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//문자열
		char var1 = '홍'; //char는 ''를 만들어 줘야함 문자열 영문이나 한글 한글자만 가능함, char는 데이터 절약하기 위해서 씀
		String var2 = "홍드로이드"; //String ""로 해줘야 하며 char를 제외한 여러문자를 이용할때 보통 다 String으로 만드는 편이고
		
		//정수열
		byte var3 = -7;//말그대로 1byte를 사용해서 -128 ~ +127까지만 가능한 숫자만 입력 가능 ''나 ""를 쓰지 않음
		short var4 = -32768;//2byte를 사용하고 -32768 ~ + 32767까지 입력이 가능한 숫자
		int var5 =  1999999999;//int가 통용적으로 정수에서 많이 쓰임 -21억 ~ +21억 까지 가능한 숫자
		long var6 = 1999999999;//long도 int 다음으로 많이 쓰임 String이랑 비슷하게 가능함;
		
		//실수열 (소수점)
		float var7 = 0.1;
	}

}
